{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/favicon.png",
      "dest": "/public/favicon.png"
    },
    {
      "src": "/apple-touch-icon.png",
      "dest": "/public/apple-touch-icon.png"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/lab-consumables",
      "dest": "/public/lab-consumables.html"
    },
    {
      "src": "/geological-consumables",
      "dest": "/public/geological-consumables.html"
    },
    {
      "src": "/safety-consumables",
      "dest": "/public/safety-consumables.html"
    },
    {
      "src": "/technical-services",
      "dest": "/public/technical-services.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}