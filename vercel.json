{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/favicon.png",
      "dest": "/favicon.png"
    },
    {
      "src": "/apple-touch-icon.png",
      "dest": "/apple-touch-icon.png"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/lab-consumables",
      "dest": "/lab-consumables.html"
    },
    {
      "src": "/geological-consumables",
      "dest": "/geological-consumables.html"
    },
    {
      "src": "/safety-consumables",
      "dest": "/safety-consumables.html"
    },
    {
      "src": "/technical-services",
      "dest": "/technical-services.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}