{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/lab-consumables",
      "dest": "/lab-consumables.html"
    },
    {
      "src": "/geological-consumables",
      "dest": "/geological-consumables.html"
    },
    {
      "src": "/safety-consumables",
      "dest": "/safety-consumables.html"
    },
    {
      "src": "/technical-services",
      "dest": "/technical-services.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 